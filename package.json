{
  "name": "react-skeleton",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "pnpm": ">=10"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "pnpm --filter web lint",
    "format": "pnpm --filter web format",
    "dev": "pnpm --filter web dev",
    "check-types": "pnpm --filter web check-types",
    "build": "pnpm --filter web build",
    "serve": "pnpm --filter web preview",
    "preview-storybook": "pnpm --filter storybook-config preview-storybook",
    "storybook": "pnpm --filter storybook-config storybook",
    "test": "pnpm --filter web test",
    "test:e2e": "pnpm --filter web test:e2e",
    "test:e2e-ui": "pnpm --filter web test:e2e-ui"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
