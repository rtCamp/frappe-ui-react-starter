{
  "name": "react-skeleton",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "pnpm": ">=10"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "dev": "turbo run dev",
    "check-types": "turbo run check-types",
    "build": "turbo run build --mode production",
    "serve": "turbo run preview",
    "preview-storybook": "turbo run preview-storybook",
    "storybook": "turbo run storybook",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "test:e2e-ui": "turbo run test:e2e-ui"
  },
  "devDependencies": {
    "turbo": "^2.5.6"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
